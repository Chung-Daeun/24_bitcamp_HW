package _02_Middle;

public class _06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		6. 중첩 for문을 이용해서 아래와 같이 출력하세요.
//		6-1)  
//		*
//		**
//		***
//		****
//		*****
		
		for(int i = 1; i <= 5; i++) {
			for(int i = 1; i <= 5; i++) {
				System.out.print("*");
			}
			System.out.println();
		}
//		6-2)
//		    *
//		   **
//		  ***
//		 ****
//		*****
//		6-3)
//		*****
//		****
//		***
//		**
//		*
//		6-4)
//		*****
//		 ****
//		  ***
//		   **
//		    *
	}

}
